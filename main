#include <iostream>

using namespace std;

int main()
{
    int array[]{3, 5, -4, 8, 11, 1, -1, 6};
    int final_array[2];
    int targetSum{10};
    int sum{};
    
    for (size_t i{0}; i < 8; i++)
    {
        for (size_t t{0}; t < 8; t++)
        {
            sum = array[i] + array[t];
            if (sum == targetSum)
            {
                final_array[0] = array[i];
                final_array[1] = array[t];
                break;
            }
        }
    }
    
    cout << final_array[0] << " " << final_array[1] << endl;
}
